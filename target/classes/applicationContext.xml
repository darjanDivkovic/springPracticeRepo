<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">
 
 <!-- We can define our beans here --> 
 <bean name="Teacher" class="com.darjanDivkovic.springTeacherProjectPractice.Teacher">
	<property name="name" value="Darjan"></property>
	<property name="surname" value="Divkovic"></property>
	<property name="salary" value="20000"></property>
 	
 </bean>
 
</beans>